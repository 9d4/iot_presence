<div id="app">
  <p>
    <code>Status: {{ wsStatus }}</code>
  </p>
  <div class="table-responsive">
    <table class="table">
      <thead>
        <tr>
          <th>RFID</th>
          <th>Name</th>
          <th>Class</th>
          <th>NIS</th>
          <th>Time</th>
        </tr>
      </thead>
      <tbody>
        <tr v-for="p in presences" :key="p._id">
          <td>{{ p.rfid }}</td>
          <td>{{ p.student.name }}</td>
          <td>{{ p.student.class }}</td>
          <td>{{ p.student.nis }}</td>
          <td>{{ p.date }}</td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
<script>
  window.presences = "<%= presences %>";
</script>
<script src="/dist/presence-realtime.js" type="module"></script>
